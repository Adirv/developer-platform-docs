var reportType = "1"
var reportInputFilter = {objectType: "KalturaReportInputFilter"};
reportInputFilter.fromDay = "20181001";
reportInputFilter.toDay = "20181031";
var pager = {objectType: "KalturaFilterPager"};
pager.pageSize = 20;
pager.pageIndex = 1;

KalturaReportService.getTable(reportType, reportInputFilter, pager)
.execute(client, function(success, results) {
  if (!success || (results && results.code && results.message)) {
    console.log('Kaltura Error', success, results);
  } else {
    console.log('Kaltura Result', results);
  }
});
